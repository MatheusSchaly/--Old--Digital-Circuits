force -freeze sim:/multiplexer/Sel 10 200
force -freeze sim:/multiplexer/Sel 11 300
force -freeze sim:/multiplexer/Sel 00 400
force -freeze sim:/multiplexer/Sel 01 500
force -freeze sim:/multiplexer/Sel 10 600
force -freeze sim:/multiplexer/Sel 11 700

