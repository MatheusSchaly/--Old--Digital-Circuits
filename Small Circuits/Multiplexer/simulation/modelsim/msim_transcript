# Loading work.multiplexer(multiplexer_arch)
add wave -position end  sim:/multiplexer/A
add wave -position end  sim:/multiplexer/B
add wave -position end  sim:/multiplexer/C
add wave -position end  sim:/multiplexer/D
add wave -position end  sim:/multiplexer/Sel
add wave -position end  sim:/multiplexer/Output
force -freeze sim:/multiplexer/D 111 0
force -freeze sim:/multiplexer/C 100 0
force -freeze sim:/multiplexer/B 001 0
force -freeze sim:/multiplexer/C 100 0
restart -f -nolist -nowave -nobreak -novirtuals -noassertions -nofcovers -noatv
add wave -position end  sim:/multiplexer/Sel
add wave -position end  sim:/multiplexer/Output
add wave -position end  sim:/multiplexer/D
add wave -position end  sim:/multiplexer/C
add wave -position end  sim:/multiplexer/B
add wave -position end  sim:/multiplexer/A
restart -f -nolist -nolog -nobreak -novirtuals -noassertions -nofcovers -noatv
force -freeze sim:/multiplexer/D 111 0
force -freeze sim:/multiplexer/C 100 0
force -freeze sim:/multiplexer/B 010 0
force -freeze sim:/multiplexer/A 001 0
force -freeze sim:/multiplexer/Sel 1 0, 0 {50 ps} -r 100
# Value length (1) does not equal array index length (2).
# 
# ** Error: (vsim-4011) Invalid force value: 1 0, 0 {50 ps}.
# 
run
run
run
force -freeze sim:/multiplexer/Sel 00 0 -cancel 100
restart -f -nolist -nolog -nobreak -novirtuals -noassertions -nofcovers -noatv
force -freeze sim:/multiplexer/D 111 0
